{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prisma:generate": "aurora && npx prisma generate",
    "prisma:build": "yarn prisma:generate 2>&1 > /dev/null",
    "prebuild": "rimraf dist && yarn prisma:build",
    "build": "nest build",
    "dev": "yarn prebuild && webpack --watch",
    "start": "node dist/server/src/main",
    "format": "prettier --config ../../.prettierrc --write \"./**/*.[jt]s?(x)\"",
    "lint": "eslint \"./**/*.[jt]s?(x)\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.3.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-fastify": "^8.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-cookie": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.6",
    "@swc/core": "^1.2.142",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify": "^3.27.2",
    "prisma": "^3.9.2",
    "prisma-aurora": "^1.3.3",
    "run-script-webpack-plugin": "^0.0.11",
    "source-map-support": "^0.5.21",
    "swc-loader": "^0.1.15",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
